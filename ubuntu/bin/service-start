#!/bin/bash

# 2015 Jon Suderman
# https://github.com/suderman/local

while read service
do
  container=$(docker ps | grep suderman/$service:latest)

  if [ -z "$container" ]; then
    echo "Starting $service"
    cd $HOME/.local/ubuntu/docker/$service && d clean && d start
  else
    echo "$service already started"
  fi

done < $HOME/.local/ubuntu/docker/services.txt
