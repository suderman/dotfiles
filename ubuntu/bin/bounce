#!/bin/bash

# 2015 Jon Suderman
# https://github.com/suderman/local

# Helper methods for prettier shell scripting - http://shelper.suderman.io
eval "$(cat ~/.local/share/shelper.sh || curl shelper.suderman.io/shelper.sh)"

if has $HOME/.local/ubuntu/docker/$1; then
  echo "Bouncing $1"
  cd $HOME/.local/ubuntu/docker/$1 && d clean && d clean-image && d start
fi
