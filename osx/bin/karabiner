#!/bin/sh

cli=/Applications/Karabiner.app/Contents/Library/bin/karabiner

$cli set repeat.initial_wait 100
/bin/echo -n .
$cli set space_cadet.right_option_to_hyper 1
/bin/echo -n .
$cli set option.tabmode_q 1
/bin/echo -n .
$cli set option.simultaneouskeypresses_vimode_f_to_fn 1
/bin/echo -n .
$cli set remap.fnletter_to_ctrlletter2 1
/bin/echo -n .
$cli set option.tabmode_hl 1
/bin/echo -n .
$cli set remap.tabmode 1
/bin/echo -n .
$cli set repeat.wait 10
/bin/echo -n .
$cli set remap.simultaneouskeypresses_vi_to_vimode 1
/bin/echo -n .
$cli set remap.simultaneouskeypresses_vimode_sd 1
/bin/echo -n .
$cli set option.tabmode_w 1
/bin/echo -n .
$cli set remap.controlL2controlL_escape 1
/bin/echo -n .
/bin/echo
